{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2021", "DOM", "DOM.Iterable"],
      "module": "ESNext",
      "moduleResolution": "bundler",
      "jsx": "react-jsx",
      "strict": true,
      "skipLibCheck": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "allowSyntheticDefaultImports": true,
      "types": ["vite/client"]
    },
    "include": ["./"]
  }
  